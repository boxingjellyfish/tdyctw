  "compileOnSave": true,
  "compilerOptions": {
    "module": "system",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "outFile": "./Scripts/game.js"
  },
  "include": [
    "TypeScript/**/*"
  ],
  "exclude": [
    "TypeScript/def/*"
  ]
}
